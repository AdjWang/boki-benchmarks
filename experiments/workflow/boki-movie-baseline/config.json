{
    "machines": {
        "bokiexp-gateway": {
            "type": "c5d.2xlarge",
            "role": "manager"
        },
        "bokiexp-client": {
            "type": "c5d.xlarge",
            "role": "client"
        },
        "bokiexp-sequencer-1": {
            "type": "c5d.2xlarge",
            "role": "worker",
            "labels": [
                "sequencer_node=true"
            ]
        },
        "bokiexp-storage-1": {
            "type": "c5d.2xlarge",
            "role": "worker",
            "mount_instance_storage": "nvme1n1",
            "labels": [
                "storage_node=true"
            ]
        },
        "bokiexp-engine-1": {
            "type": "c5d.2xlarge",
            "role": "worker",
            "labels": [
                "engine_node=true"
            ]
        }
    },
    "services": {
        "zookeeper": {
            "placement": "bokiexp-gateway"
        },
        "zookeeper-setup": {
            "placement": "bokiexp-gateway"
        },
        "boki-controller": {
            "placement": "bokiexp-gateway"
        },
        "boki-gateway": {
            "placement": "bokiexp-gateway"
        },
        "boki-storage": {
            "placement_label": "storage_node",
            "replicas": 1
        },
        "boki-sequencer": {
            "placement_label": "sequencer_node",
            "replicas": 1
        },
        "boki-engine": {
            "placement_label": "engine_node",
            "replicas": 1
        },
        "Frontend-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "CastInfo-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "ReviewStorage-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "UserReview-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "MovieReview-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "ComposeReview-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "Text-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "User-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "UniqueId-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "Rating-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "MovieId-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "Plot-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "MovieInfo-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        },
        "Page-service": {
            "placement_label": "engine_node",
            "replicas": 1,
            "need_aws_env": true,
            "mount_certs": true
        }
    },
    "aws_region": "us-east-2"
}